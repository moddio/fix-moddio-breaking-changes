{
	"main": "dist/index.js",
	"scripts": {
		"build": "npx esbuild index.ts --bundle --outfile=dist/bundle.js --platform=browser --external:fs --external:graceful-fs",
		"fixDebris": "npx -y tsx ./src/debris.ts",
		"fixScale": "npx -y tsx ./src/scale.ts",
		"fixFixture": "npx -y tsx ./src/fixture.ts",
		"fixDamping": "npx -y tsx ./src/damping.ts",
		"offsetColliders": "npx -y tsx ./src/offsetColliders.ts",
		"lockXYRotations": "npx -y tsx ./src/lockXYRotations.ts",
		"downloadTemplates": "ts-node-esm ./src/downloadTemplates.ts",
		"deploy": "pnpm run build && gh-pages -d dist"
	},
	"devDependencies": {
		"gh-pages": "^6.3.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@types/jsonfile": "^6.1.4",
		"axios": "^1.7.9",
		"jsonfile": "^6.1.0",
		"rfdc": "^1.4.1"
	}
}
