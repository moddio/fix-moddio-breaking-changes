{
	"main": "dist/index.js",
	"scripts": {
		"build": "npx esbuild index.ts --bundle --outfile=dist/bundle.js --platform=browser --external:fs --external:graceful-fs",
		"convert": "npx -y tsx ./src/convert.ts",
		"downloadTemplates": "ts-node ./src/downloadTemplates.ts",
		"backupTom": "ts-node ./src/backupTom.ts",
		"deploy": "pnpm run build && gh-pages -d dist",
		"removeUnwantedProperties": "npx -y tsx ./src/removeUnwantedProperties.ts",
		"tsdocs:generate": "npx -y tsx ./tsdocs/generateTSDocs.ts"
	},
	"devDependencies": {
		"gh-pages": "^6.3.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@types/jsonfile": "^6.1.4",
		"axios": "^1.7.9",
		"dotenv": "^16.6.1",
		"jsonfile": "^6.1.0",
		"rfdc": "^1.4.1"
	}
}
